{
  "name": "Google Calendar Daylight",
  "version": "1.0.10",
  "manifest_version": 2,
  "description": "Modifies Google Calendar to visualize daylight hours in your timezone.",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "name": "Google Calendar Daylight",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Google Calendar Daylight"
  },
  "content_scripts": [{
    "js": ["background.js", "daylight.js", "jquery.min.js", "suncalc/suncalc.js"],
    "matches": [
      "http://www.google.com/calendar/*", 
      "https://www.google.com/calendar/*", 
      "http://*.google.com/calendar/*", 
      "https://*.google.com/calendar/*"
    ]
    }],
  "permissions": [ "tabs", "https://www.google.com/calendar/*" ]
}